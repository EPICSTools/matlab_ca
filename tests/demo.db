# Looks a bit like the 'excas' data, but full records
#
#   softIoc -s -d demo.db

record(calc, "ramp")
{
   field(DESC, "A Ramp")
   field(SCAN, "1 second")
   field(CALC, "A<1000?A+1:0")
   field(INPA, "ramp")
}

record(calc, "fred")
{
   field(SCAN, "1 second")
   field(CALC, "RNDM")
   field(FLNK, "alan")
}

record(calc, "janet")
{
   field(SCAN, ".1 second")
   field(CALC, "RNDM")
}

record(compress, "alan")
{
   field(ALG, "Circular Buffer")
   field(INP, "fred")
   field(NSAM, "100")
}
